Include config.d/*

Host *
    ServerAliveInterval 25

    # Check SSHFP DNS record
    VerifyHostKeyDNS yes

    # Fix terminfo for kitty:
    SetEnv TERM=xterm-256color

    # Remove bad ciphers from default list
    HostKeyAlgorithms -ssh-rsa
    # MACs without "-etm" will do mac-then-encrypt
    MACs -umac-64-etm@openssh.com,hmac-sha1-etm@openssh.com,umac-64@openssh.com,umac-128@openssh.com,hmac-sha2-256,hmac-sha2-512,hmac-sha1

    # SSH Multiplexing
    #ControlPath ~/.ssh/controlmasters/%r@%h:%p
    #ControlMaster auto
    #ControlPersist 10m

Host forside
    HostName xn--sb-lka.org
    User root

Host hetzner
    HostName 95.216.212.222
    User root

Host matrix
    HostName 135.181.35.20
    User root

Host pyjamas
    HostName pyjam.as
    User nsq
    IdentityFile ~/.ssh/id_rsa-old

Host pfbitter
    HostName ssh.phx.nearlyfreespeech.net
    User nicolais_pfbitter
    IdentityFile ~/.ssh/id_rsa-old

Host hackerlab
    HostName 192.168.234.145
    User hackerlab
